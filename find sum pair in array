#include<stdio.h>

int main(){

int sum; 

scanf("%d",&sum); 

int N; 

scanf("%d",&N); 
  

int array[N];

  

    

 int condition=0;

    for(int i=0;i<N;i++){

    

    scanf("%d", &array[i]); 

        

    }int i; 

    int j ; 
    

for( i=0; i<N-1;i++){

      for( j=i+1; j<N;j++){

      if (array[i]+array[j]==sum){condition=1; 

                                 }

    

       

                                  

     

          

  

    }
          

      }

if(condition=1){printf("1");}

    else{printf("0");}

}



